{
  "name": "mk-springboot-web",
  "version": "1.0.0",
  "description": "Spring Boot Angular Sample application setup",
  "scripts": {
    "cleanup": "rimraf dist/",
    "start": "npm run webpack:dev",
    "serve": "npm run start",
    "build": "npm run webpack:prod",
    "webpack:dist": "webpack --mode development --config webpack/webpack.config.dev.js --progress --inline --hot --profile --port=9060 --watch-content-base",
    "webpack:dev": "webpack-dev-server --mode development --config webpack/webpack.config.dev.js --progress --inline --hot --profile --port=9060 --watch-content-base",
    "webpack:prod": "npm run cleanup && webpack --mode production --config webpack/webpack.config.prod.js --profile"
  },
